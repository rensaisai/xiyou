.weui-cells{
  display: flex;
  align-items: center;
}
.item-index{
  text-align: center;
}
page{
  background: #f3f7f7;
}
.weui-uploader__img{
  width: 77px;
  height: 77px;
  border: 1px solid #D9D9D9;
}
.list_box{
  background: #fff;
  width: 100%;
  position: relative;
}
.select_box{
  background: #fff;
  width: 75%;
  position: relative;
}
.select{
  box-sizing: border-box;
  width: 100%;
  height: 70rpx;
  border:1px solid #efefef;
  border-radius: 8rpx;
  display: flex;
  align-items: center;
  padding: 0 20rpx;
}
.select_text{
  font-size: 30rpx;
  flex: 1;
}
.select_img{
  width: 40rpx;
  height: 40rpx;
  display: block;
  transition:transform 0.3s;
}
.select_img_rotate{
  transform:rotate(180deg); 
}
.option_box{
  position: absolute;
  top: 70rpx;
  width: 100%;
  border:1px solid #efefef;
  box-sizing: border-box;
  overflow: hidden;
  overflow-y: auto;
  border-top: 0;
  background: #fff;
  transition: height 0.3s;
  z-index: 999;
}
.option{
  display: block;
  line-height: 40rpx;
  font-size: 30rpx;
  border-bottom: 1px solid #efefef;
  padding: 10rpx;
}
.page{
  position: relative;
}
.button{
  position: absolute;
  right: 0;
  top: 35rpx;
  width: 20%;
  height: 70rpx;
  font-size: 20rpx;
  line-height: 70rpx;
}
.picker{
  width: 100%;
  height: 40px;
  background-color: #ffffff;
  border-top: 0.1px solid #D8D8D8;
  border-bottom: 0.1px solid #D8D8D8;  
  padding: 0 20px;
  line-height: 40px;
  color: #888888;
}
.page-section{
  margin-bottom: 20rpx;
}
.register{
  margin:50rpx;
}
.logo-head{
  width: 100%;
  text-align: center;
}
.logo-img{
  width:100px;
  height:100px;
  border-radius: 20rpx;
  margin:20rpx;
}
textarea {
    width: 700rpx;
    padding: 25rpx 0;
}
.textarea-wrp {
    padding: 0 25rpx;
    background-color: #fff;
}
.radio-float{
  width:84%;
