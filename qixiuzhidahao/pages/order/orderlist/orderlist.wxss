.index{
  width: 100%;
  height:auto;
}
.list-box{
  width: 100%;
  height: 130rpx;
  background: #ffffff;
  border-bottom: 1rpx solid #E9E9E9;
  display: flex;
  justify-content: space-between;
  padding:0 20rpx;
  box-sizing: border-box;
}
.vhinformation-left{
  width: auto;
  height: 130rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  font-size: 26rpx;
  color: #000000;
  font-weight: 600;
  padding: 10rpx 0;
  box-sizing: border-box;
}
.right{
  width: auto;
  font-size: 26rpx;
  color: #1296db;
  font-weight: 600;
  line-height: 130rpx;
  text-align: center;
}