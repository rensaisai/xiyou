@import '/pages/travel/watch/watch.wxss';
.index{
  width: 100%;
  height: auto;
}
.navs{
  width: 100%;
  height: 110rpx;
}
.swpier-box{
  width: 100%;
  height: 550rpx;
  display: flex;
  flex-wrap:wrap;
}
.todays {
  height: 20rpx;
}
.time{
  font-size: 26rpx;
  color: #9D9D9D;
}
.text{
  font-size: 26rpx;
  color: #E3062A;
}
.nav-left{
  display: flex;
  align-items: center;
}
.grid{
  height: 90rpx;
  text-align: center;
  line-height: 80rpx;
  /* float: left; */
}
.intraday{
 text-align: center;
}
.hot{
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.tuxedo-box{
  width: 100%;
  height: 88rpx;
  background: #ffffff;
}
.tuxedo{
  margin: 0 auto;
  width: 80%;
  height: 88rpx;
  border-top: 1rpx dashed #AEAEAE;
  text-align: center;
  line-height: 88rpx;
  color: #AEAEAE;
  font-size:30rpx; 
}
.color{
  color:#DDDDDD;
}
.outing{
  width: 100%;
  height: auo;
  padding:  80rpx 20rpx 20rpx;
  box-sizing: border-box;
}
.outing-box{
  width: 100%;
  height: 270rpx;
  background: #ffffff;
  border-radius:10rpx; 
  padding: 0 15rpx;
  box-sizing: border-box;
}
.outing-icon-box{
  width: 100%;
  height: 80rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.outing-left{
  display: flex;
  align-items: center;
}
.icon{
  width: 64rpx;
  height: 46rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.select{
 font-size: 28rpx;
 color: #000000;
 margin-left:10rpx; 
}
.select-box{
  margin-top:30rpx; 
}
.outing-select-box{
  width: 100%;
  height: 80rpx;
  display: flex;
  justify-content: space-between;
}
.outing-select-box-icon{
  width: 44rpx;
  height: 44rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.outing-select-box-left{
  width: 270rpx;
  height: 80rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.adult{
  font-size: 30rpx;
  color: #676767;
}
.people{
  font-size: 26rpx;
  color: #E3062A;
}
.number{
  font-size: 26rpx;
  color:#676767; 
}
.outing-select-box-right{
  width: 170rpx;
  height: 80rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.reduce{
  width: 48rpx;
  height: 48rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.plus{
  width: 48rpx;
  height: 48rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.add{
  width: 48rpx;
  height: 48rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}