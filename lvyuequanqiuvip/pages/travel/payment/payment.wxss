.index{
  width: 100%;
  height: 100%;
  background: #f1f1f3;
}
.box{
  width: 100%;
  height: 344rpx;
  background: #f39400;
}
.success{
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.success-img{
  width:40rpx;
  height: 40rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.success-text{
  font-size: 40rpx;
  color: #ffffff;
  font-weight: 700;
  padding-left: 10rpx;
}
.peice{
  width: 100%;
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  font-size: 30rpx;
  color: #ffffff;
}
.button{
  width: 100%;
  height: 140rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.block{
  width: 100%;
  height: 74rpx;
}
.button-left{
  width: 180rpx;
  height: 60rpx;
  border-radius:30rpx;
  border: 1rpx solid #ffffff;
  font-size: 28rpx;
  color: #ffffff;
  text-align: center;
  line-height: 60rpx; 
}
.advertising{
  width: 95%;
  height: 144rpx;
  /* background:rgba(255,255,255,1);
  border:1px solid rgba(241, 241, 239, 1);
  box-shadow:0px 2px 3px 0px rgba(240,237,237,0.44); */
  border-radius:20px;  
  margin: -72rpx auto 0; 
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
/* .advertising-top{
  font-size: 44rpx;
  color: #AD0315;
} */
.xian{
  width: 45%;
  height: 2rpx;
  border-radius:1rpx;
  background: #AD0315; 
}
.advertising-bottom{
  font-size:20rpx;
  color: rgba(173,3,21,1); 
}
.recommend-box{
  width: 100%;
  height: auto;
  float: left;
  overflow: hidden;
  padding-bottom:20rpx; 
}
.recommend{
  width: 346rpx;
  height: 530rpx;
  background: #ffffff;
  border-radius:10rpx; 
  float: left;
  margin-left:20rpx;
  margin-top: 20rpx; 
}
.recommend-img{
  width: 100%;
  height: 348rpx;
  background: hotpink;
  border-radius:10rpx; 
}
.goods-name{
  width: 100%;
  height: 50rpx;
  font-size: 30rpx;
  color:#383838;
  line-height: 50rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
 overflow: hidden;
 padding: 0 10rpx;
 box-sizing: border-box;
}
.goods-brief{
  width: 100%;
  height: 60rpx;
  font-size: 20rpx;
  color: #383838;
  display: -webkit-box;
-webkit-box-orient: vertical;
-webkit-line-clamp: 2;
overflow: hidden;
padding: 0 10rpx;
 box-sizing: border-box;
}
.goods-price{
  width: 100%;
  height: 70rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #E3062A;
  font-size: 30rpx;
  padding: 0 10rpx;
 box-sizing: border-box;

}
.goods-price-right{
  font-size: 22rpx;
  color:#9B9A9B;
}
.goods-price-img{
  width: 46rpx;
  height: 8rpx;
  background: red;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}