.index{
  width: 100%;
  height: auto;
  background: #f1f1f3;
  padding: 20rpx;
  box-sizing: border-box;
}
.box{
  width: 100%;
  height: auto;
  background: #ffffff;
  border-radius:10rpx; 
  padding: 0 20rpx;
  box-sizing: border-box;
}
.serve{
  width: 100%;
  height: 110rpx;
  border-bottom:1rpx solid #f1f1f3; 
  display: flex;
  align-items: center;
}
.serve-img{
  width: 40rpx;
  height: 37rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.serve-text{
  font-size: 28rpx;
  color: #898888;
  padding-left:15rpx; 
}
.standard{
  width: 100%;
  height: auto;
  padding: 40rpx 0;
}
.standard-text{
  font-size: 28rpx;
  color: #656565;
  line-height: 46rpx;
}
.boxs{
  margin-top: 20rpx;
}
.img{
   width: 40rpx;
  height: 37rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.serve-imgs{
  width: 40rpx;
  height: 37rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
  }