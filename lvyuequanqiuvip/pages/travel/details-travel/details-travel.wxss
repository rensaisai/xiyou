@import '/pages/travel/hi-details/hi-details';
.index{
  width: 100%;
  height: 100%;
  background: #f1f1f3;
  padding: 0 20rpx; 
  box-sizing: border-box;
}
.dinner{
  width: 110rpx;
  height: 45rpx;
  padding-left: 10rpx;
  text-align: justify;
}
.dinners{
  width: 138rpx;
  height: 45rpx;
  padding-left: 10rpx;
}
.maohao{
  padding-right: 16rpx;
}
.dinner::after{
  width: 100%;
  display: inline-block;
  content: '';  
}
.breakfast{
  width: 100%;
  height: auto;
}
.firstly-xian1:last-child{
  display: none;
}
.box{
  position: fixed;
  top: 0;
  width: 100%;
  height: 116rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 999;
}
.block{
  width: 95%;
  height: 116rpx;
}
.numbe-text{
  width: 120rpx;
  height: 76rpx;
  font-size:30rpx; 
  color: #ffffff;
  text-align: center;
  line-height: 76rpx;
  margin: 0 auto;
}
.numbe{
  width: 241rpx;
  height: 76rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.sky{
  width: 100%;
  height: auto;
}
.sky-box{
  width: 100%;
  height: auto;
  background: #ffffff;
  border-radius:10rpx; 
  padding: 0 20rpx 20rpx;
  box-sizing: border-box; 
}
.first-box{
  padding-top: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}
.slide-box{
  width: 100%;
  height: 280rpx;
  white-space: nowrap;
  overflow-x: scroll;
  overflow-y:hidden; 
}
.slide-img{
  display: inline-block;
  width: 498rpx;
  height: 280rpx;
  /* background: #F3F3F3; */
  border-radius:10rpx;
  margin-left: 20rpx;
}
.slide-img:nth-child(1){
margin-left: 0rpx;
}
.stay-box{
  width: 100%;
  height: auto; 
  padding: 30rpx 0;
  border-bottom: 1rpx solid #E0E0E0;
}
.stay-eat{
  height: auto;
  display: flex;
  align-items: center;
  font-size: 28rpx;
  color: #898888;
}
.eat-img{
  width: 48rpx;
  height: 45rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.eat-text{
  flex-grow: 1;
  width: 100%;
  height: auto;
  padding-left:10rpx;
  color: #656565;
  display: flex;
}
.eat-imgs{
  width: 48rpx;
  height: 45rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.briefs{
  width: 100%;
  height: auto;
  font-size: 28rpx;
  color: #656565;
  line-height: 48rpx;
  padding: 30rpx 0 30rpx;
  box-sizing: border-box;
}
.kalendar{
  width: 100%;
  height: 116rpx;
  display: flex;
}
.kalendar-img{
  width: 70rpx;
  height: 116rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.kalendar-text{
  font-size: 28rpx;
  line-height: 116rpx;
  color: #E41342;
  padding-left:5rpx; 
}
.commodity_screens{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background:linear-gradient(to right, rgba(242,102,140,0.9),rgba(242,131,135,0.8));
  z-index: 1000;
  color: #fff;
  padding: 100rpx 0;
  box-sizing: border-box;
}
.scroll{
  width: 100%;
  height: 100%;
  overflow:hidden;
  overflow-y:scroll; 
}
.firstly{
  width: 100%;
  height: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size:24rpx; 
}
.yuan-box{
  width: 80rpx;
  height: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.firstly-yuan{
  width: 50rpx;
  height: 50rpx;
  background: #ffffff;
  border-radius:50%; 
  color: #F75A57;
  text-align: center;
  line-height: 50rpx;
  font-size:30rpx; 
  font-weight: 700;
}
.firstly-xian{
  width: 50%;
  height: 56rpx;
  border-right:1rpx solid #ffffff; 
  margin-top: 15rpx;
}
.firstly-xian1{
  width: 50%;
  height: 56rpx;
  border-right:1rpx solid #ffffff; 
  margin-bottom: 15rpx;
}
.trip{
  width: 100%;
  height: 30rpx;
  display: flex;
}
.trip-text1{
  width: 45%;
  height: 30rpx;
  font-size:30rpx; 
  line-height: 30rpx;
  display: flex;
  flex-direction: row-reverse;
}
.trip-text{
  width: 45%;
  height: 30rpx;
  font-size:30rpx; 
  line-height: 30rpx;
  display: flex;
}
.trip-box{
  width: 80rpx;
  height: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center; 
}
.trip-yuans{
  width: 22rpx;
  height: 22rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.trip-yuan{
  width: 36rpx;
  height: 22rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}