@import "/pages/common/index.wxss";

.mine-head{
  padding-top:30rpx;
  color: #ffffff;
}
.nickname{
  color: #ffffff;
}
.user-statics{
  display: flex;
  margin-top: 4px;
}
.statics{
  margin:1rpx;
  box-sizing: border-box;
  width: 50%;
  padding: 15rpx 0rpx;
  display: flex;
  color:#ffffff;
  font-size: 28rpx;
  justify-content: center;
  align-items: center;
}
.huiyuan{
  width:30%;
  padding: 8rpx;
  background-color: #e60012;
  box-sizing: border-box;
  border-radius:25px; 
  font-size: 24rpx;
  margin: 0 auto;
}
.mineback{
  width: 100%;
  height:436rpx;
  background: url(data:image/png;base64,) no-repeat 100% 100%/100%;
}
.mineback-img{
  width: 100%;
  height:430rpx;
  position: absolute;
  z-index: -1;
}
.headimg{
  width: 140rpx;
  height:140rpx;
  border-radius: 70rpx;
  margin: 0 auto;
  overflow: hidden;
}
open-data{
  width: 140rpx;
  height:140rpx;
  border-radius: 70rpx;
}
.list-icon{  
  width: 50rpx;
  height:50rpx;
}
.item-left{
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 0 0 10rpx;
  box-sizing: border-box; 
}