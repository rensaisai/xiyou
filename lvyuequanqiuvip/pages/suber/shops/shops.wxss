@import "/pages/common/index.wxss";
.box{
  display: flex;
  justify-content: space-between;
  padding: 10px 5px;
}
.index-box{
  width: 100%;
  height: auto;
  /* margin-top:80rpx;  */
  /* padding: 0 0 45rpx; */
}
.index-lists{
  width: 100%;
  height: 85rpx; 
}
/* .kind-list-items{
  width: 100%;
  height: 100%; 
  overflow-y:scroll; 
} */
.swiper-box{
  width: 100%;
  height: 350rpx;
}
.swiper-img{
  width: 100%;
}
.index-list{
  width: 100%;
  height: 210rpx;
  background-color: #ffffff;
  display: flex;
}
.index-list .shop{
  width: 33%;
  height: 200rpx;
  /* background: red; */
  display: flex;
  flex-direction:column;
  /* justify-content: space-around; */
  align-items: center;
}
.shop-img{
  display: flex;
  align-items: center;
  justify-content: space-around;
  width: 150rpx;
  height: 150rpx;
}
.bao-img{
  width: 120rpx;
  height: 113rpx;
}
.txt{
  font-size: 30rpx;
  color: #0d0d0d;
  font-weight: 700;
}
.list{
  position: relative;
  width: 100%;
  height: auto;
  margin-top: 10rpx;
  background: #ffffff;
}
.list-img{
  width: 100%;
  height: 366rpx;
  background: #ffffff;
}
.list-count{
  position: absolute;
  right: 20rpx;
  margin-top: -60rpx;
  width: auto;
  height: 35rpx;
	background-color: #9d9d9d;
	border-radius: 18px;
  font-size: 26rpx;
  color: #ffffff;
  line-height: 35rpx;
  padding: 0 10rpx;
}
.list-box{
  width: 100%;
  height: auto;
  background-color: #ffffff;
}
.goods-img{
   width: 100%;
   font-size: 16px;
   line-height: 26px;
}
.list-name{
  /* height: 77.5rpx; */
  height: auto;
  display: flex;
  /* flex-grow: 1; */
  line-height: 77.5rpx;
  padding: 0 20rpx;
  font-size: 32rpx;
  color: #0d0d0d;
  font-weight: 700;
}
.shops{
  justify-content: space-between;
}
.list-price{
  font-size: 32rpx;
  color: #e51337;
}
.list-cost{
  font-size: 28rpx;
}
.list-money{
   font-size: 28rpx;
   color: #878787;
   font-weight: normal;
   text-decoration:line-through;
}
.list-btn{
  width: 150rpx;
	height: 60rpx;
	background-color: #e51337;
	border-radius: 5rpx;
  font-size: 28rpx;
  color: #ffffff;
  line-height: 60rpx;
  text-align: center;
}
.proprietary{
  width: 64rpx;
  height: 32rpx;
  background: #e51337;
  border-radius:6rpx;
  font-size:22rpx;
  color: #ffffff;
  font-weight: 600;
  margin-top: 12rpx; 
  text-align: center;
  line-height: 32rpx;
}
.promotion{
  width: 62rpx;
  height: 30rpx;
  border-radius:6rpx; 
  border: 1rpx solid #e51337;
  color: #e51337;
  font-size: 22rpx;
  font-weight: 600;
  margin-top: 12rpx;
  text-align: center;
  line-height: 30rpx;
}
.hot{
  margin-left: 5rpx;
}
