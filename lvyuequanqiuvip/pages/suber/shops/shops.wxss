.menu_box{
 position: fixed;
 top: 0;
 width: 100%;
 height: 80rpx;
 border: 2rpx solid #cccccc;
 z-index: 999;
 background: #ffffff;
 white-space:nowrap;
 overflow-x:scroll;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.borders{
 border-bottom: 8rpx solid #f39400;
 color: #f39400;
}