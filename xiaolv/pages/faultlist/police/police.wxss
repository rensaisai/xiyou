page{
 background: #ffffff;
}
.index{
  width: 100%;
  height: auto;
}
.data-box{
  width: 100%;
  height: 150rpx;
  border-bottom:1rpx solid #F0ECECFF; 
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.time-box{
  width: 245rpx;
  height: 70rpx;
  background: #F0F0F0FF;
  border-radius:8rpx; 
  font-size: 30rpx;
  color: #333333FF;
  padding: 0 20rpx;
  box-sizing:border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.zhi-text{
  font-size: 26rpx;
  color: #000000;
  font-weight: 600;
}
.inquire-box{
  width: 124rpx;
  height: 70rpx;
  border-radius:8rpx;
  background: #E84A2EFF;
  font-size: 30rpx;
  color: #ffffff;
  font-weight: 600;
  text-align: center;
  line-height: 70rpx;  
}
.time-icon{
  width: 20rpx;
  height: 20rpx;
}
.list-box{
  width: 100%;
  height: 110rpx;
  border-bottom:1rpx solid #F0ECECFF;
  padding: 0 20rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}
.left{
  font-size: 30rpx;
  color: #666666FF;
}
.right{
  flex: 1;
  margin-left:70rpx;
  font-size: 26rpx;
  color: #000000FF;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap; 
}