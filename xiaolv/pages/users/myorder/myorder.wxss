@import '/pages/progect/project.wxss';
.next-box{
  position: fixed;
  top: 0;
  left: 0;
  width: 80rpx;
  height: 92rpx;
  display: flex;
  align-items: center;
  padding-left: 20rpx;
}
.next{
  width: 34rpx;
  height: 40rpx;
}
.tab-box{
  position: fixed;
  top:0;
  left: 0;
  width: 100%;
  height: 92rpx;
  background: #ffffff;
  display: flex;
  justify-content: space-around;
  align-items: center;
  font-size: 34rpx;
  color: #232323;
  z-index: 999;
}
.pitch{
  color: #E84A2E;
  font-weight: 600;
  border-bottom: 2rpx solid #E84A2E;
}
.nav-boxs{
  position: fixed;
  top: 94rpx;
  left: 0;
  width: 100%;
  height: 92rpx;
  background: #ffffff;
  z-index: 999;
}
.navs{
  width: 20%;
  height: 91rpx;
  display: inline-block;
  font-size: 30rpx;
  color: #666666;
  text-align: center;
  font-weight: 600;
  line-height: 91rpx;
  border-bottom:1rpx solid #ffffff; 
}
.block{
  width: 100%;
  height: 92rpx;
}
.nav-selected{
  color: #E84A2E;
  border-bottom:1rpx solid #E84A2E; 
}
.list{
  width: 100%;
  height: auto;
  margin-top: 2rpx;
}
.list-statse{
  width: 100%;
  height: 90rpx;
  background: #ffffff;
  padding: 0 20rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.list-time-left{
  display: flex;
  align-items: center;
}
.list-ice{
  width: 30rpx;
  height: 30rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.lisyt-time{
  margin-left: 18rpx;
  font-size: 30rpx;
  color: #666666;
  font-weight: 600;
}
.list-statue-right{
  font-size: 30rpx;
  color: #E8862E;
  font-weight: 600;
}
.list-box{
  width: 100%;
  height: auto;
  background: #ffffff;
  margin-top: 2rpx;
}
.list-price{
  width: 100%;
  height: 70rpx;
  display: flex;
  flex-direction: row-reverse;
  align-items: center;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.maintenance-list{
  border-bottom:0rpx;
  padding-left:60rpx;
  box-sizing: border-box;  
}
.clearfloat{
  clear:both;
  }
  .prices{
    font-size: 32rpx;
    color: #E84A2E;
    font-weight: 600;
  }
  .heji{
    font-size: 30rpx;
    color:#000000;
    font-weight: 600;
  }
  .describe{
    font-size: 28rpx;
    color: #a7a7a7;
    margin-right:20rpx;
  }
  .btn-status{
    width: 100%;
    height: 94rpx;
    display: flex;
    flex-direction: row-reverse;
    align-items: center;
    padding: 0 20rpx;
    box-sizing: border-box;
  }
  .show{
    display: none;
  }
  .btn{
    width: 150rpx;
    height: 60rpx;
    border:1rpx solid #E84A2E;
    border-radius:6rpx;
    font-size: 28rpx;
    color: #E84A2E;
    text-align: center;
    line-height: 60rpx;
  }
  .invoice{
    margin-right: 20rpx;
  }
  .selected-style{
  background: #E84A2E;
  color: #ffffff;
}