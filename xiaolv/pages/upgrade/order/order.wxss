
@import '/pages/upgrade/must-buy/must-buy.wxss';
.index{
  width: 100%;
  height: auto;
  padding-bottom:120rpx; 
}
.box{
  width: 100%;
  height: 108rpx;
  background: #ffffff;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.coupon-box{
  height: 260rpx;
  display: flex;
  align-items: center;
  background: #ffffff;
  margin-top:20rpx; 
  padding:0 10rpx;
  box-sizing: border-box; 
}
.icon{
  width: 30rpx;
  height: 30rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.box-left{
  width: auto;
 height: 108rpx;
 display: flex;
 align-items: center;
}
.texts{
  width: auto;
  font-size: 30rpx;
  color: #000000;
  font-weight: 600;
  margin-left: 20rpx;
}
.background{
  width: 100%;
  height: 8rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.block-box{
  width: 100%;
  height: 460rpx;
  background: #ffffff;
  margin-top:30rpx; 
  display: flex;
  align-items: center;
  padding:0 20rpx;
  box-sizing: border-box; 
}
.blocks{
  width: 100%;
  height: 408rpx;
  background: url("https://www.cmspq.xyz/cmsweb/imgs/SmallProcedures/11.png") no-repeat center center/cover;
}
.block{
  width: 100%;
  height: 408rpx; 
}
.order{
  width: 100%;
  height: 110rpx;
  background: #ffffff;
  margin-top:4rpx; 
  padding: 0 20rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 32rpx;
  color: #000000;
  font-weight: 600;
}
.price-icon{
  font-size: 24rpx;
}
.price{
   font-size: 34rpx;
   color: #000000;
}
.money{
  color: #E84A2E;
}
.num{
  width: 214rpx;
  height: 60rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.nums{
  width: 90rpx;
  height: 60rpx;
  background: #EEEEEE;
  text-align: center;
  line-height: 60rpx;
}
.subtract{
  width: 60rpx;
  height: 60rpx;
  background: #EEEEEE;
  display: flex;
  justify-content: center;
  align-items: center;
}
.img{
