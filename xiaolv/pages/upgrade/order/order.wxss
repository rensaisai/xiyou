  display: flex;
  align-items: center;
}
.WeChat-text{
 font-size: 30rpx;
 color: #000000;
 margin-left: 20rpx;
}
.select{
  width: 36rpx;
  height: 36rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.btn{
  position: fixed;
  bottom: 20rpx;
  left: 20rpx;
  width: 95%;
  height: 88rpx;
  border-radius:8rpx;
  background: #E84A2E;
  font-size: 30rpx;
  color: #ffffff;
  line-height: 88rpx;
  text-align: center;
}
button {
