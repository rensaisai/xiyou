  color: #666666;
  margin-left: 15rpx;  
}
.float{
   width: 100%;
  height: 110rpx;
  background: #ffffff;
  margin-top:4rpx; 
  padding: 0 20rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: row-reverse;
  align-items: center;
}
.texts{
  color: #000000;
}
.names{
  margin-right:25rpx; 
}
.payment{
