  background:url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABgAAAAYCAYAAADgdz34AAAAR0lEQVRIS2NkIBLMnDnzP7LS9PR0RmK0EqUIZNCoBQSDczSIRoMIZwgwoqcOgmFFooJRCwgG2GhhNxpEiBAYLa4JpoZBG0QAaF9bQcq7gxEAAAAASUVORK5CYII=) no-repeat center center/cover;
}
.orders{
  width: 100%;
  height: auto;
  background: #ffffff;
  padding: 0 20rpx 20rpx;
  box-sizing: border-box; 
  margin-top: 4rpx;
}
.notice{
  font-size: 32rpx;
  line-height: 100rpx;
  color: #000000;
  font-weight: 600;
}
.describes{
  display: flex;
  align-items: center;
  font-size:26rpx; 
}
.yuan{
