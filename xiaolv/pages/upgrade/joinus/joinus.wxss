page{
  background: #ffffff;
}
.index{
  width: 100%;
  height: auto;
  padding-bottom: 20rpx;
}
.icon-box{
  width: 100%;
  height: 500rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.icon-img{
  width: 140rpx;
  height: 140rpx;
}
.title-box{
  width: 100%;
  height: 70rpx;
  border-bottom:1rpx solid #E9E9E9;  
}
.title{
  font-size: 26rpx;
  color: #99A4B1;
  font-weight:599;
  padding-left: 20rpx;
  line-height: 70rpx;
}
.img-box{
  width: 100%;
  height: auto;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.image{
  width: 100%;
  height: 400rpx;
  margin-top: 20rpx;
  border: 1rpx dashed #99A4B1; 
  display: flex;
  justify-content: center;
  align-items: center;
}
.img1{
  width: 100%;
  height: 400rpx;
  margin-top: 20rpx;
  border: 1rpx dashed #99A4B1; 
  display: flex;
  justify-content: center;
  align-items: center;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.uploading-box{
  width: 116rpx;
  height: 170rpx;
}
.uploading{
  width: 116rpx;
  height: 116rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.uploading-text{
  font-size: 26rpx;
  color: #000000;
  text-align: center;
  font-weight: 700;
  line-height: 60rpx;
}
.img2{
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.text-box{
  width: 100%;
  height: 80rpx;
  margin:  0 auto;
  font-size: 28rpx;
  color: #99A4B1;
  display: flex;
  align-items: center;
}
.text{
  padding-left:10rpx; 
}
.icon{
  width: 28rpx;
  height: 28rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.signature-box{
  width: 100%;
  height: 400rpx;
  margin-top: 20rpx;
  border: 1rpx dashed #99A4B1; 
}
.apply{
  height: 100rpx;
  background: #eaeff3;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.delbutton{
  width: 110rpx;
  height: 60rpx;
  background-color: #E84A2E;
  font-size: 26rpx;
  color: #ffffff;
  font-weight: 700;
  text-align: center;
  line-height: 60rpx;
  border-radius:5rpx; 
}
.canvas{
  width: 100%;
  height: 300rpx;
}
.recommend-box{
  width: 100%;
  height: 200rpx;
  display: flex;
  align-items: center;
}
.recommend-boxs{
  width: 100%;
  height: 120rpx;
  display: flex;
  align-items: center;
}
.recommend{
  width: 100%;
  height: 88rpx;
  border-radius:8rpx;
  background: #F5F7F9;
  display: flex;
  flex-grow:1; 
  border: 1rpx solid #CED5E0; 
}
.recommend-text{
  width: 380rpx;
  font-size: 30rpx;
  color: #000000;
  font-weight: 600;
  padding-left:20rpx;
  line-height: 88rpx; 
}
.input{
  width: 100%;
  height: 88rpx;
}
.method{
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
}
.method-box{
width: 100%;
height: 38rpx;
display: flex;
align-items: center;
}
