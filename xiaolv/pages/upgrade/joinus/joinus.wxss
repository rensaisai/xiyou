font-size: 28rpx;
font-weight: 700;
color: #000000;
padding-left:10rpx; 
}
.method-imgs{
   width: 38rpx;
  height: 38rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.texts{
 color: #99A4B1;
}
.link{
  color: #F29484;
}
.btn{
  width: 100%;
  height: 88rpx;
  background: #E84A2E;
  border-radius:8rpx; 
  color: #ffffff;
  font-size:30rpx;
  text-align: center;
  line-height: 88rpx; 
}
.commodity_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 1000;
  color: #fff;
}
.commodity_attr_box {
  width: 100%;
  height: 100%;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 2000;
  overflow: hidden;
  display: flex;
  justify-content: center;
  align-items: center;
}
.box{
  width: 540rpx;
  height: 325rpx;
  border-radius:10rpx;
  position: relative;
  /* background: url("https://www.cmspq.xyz/cmsweb/imgs/ads/cardBackGround.png") no-repeat center center/cover; */
}
.recommend-person{
  position: absolute;
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 34rpx;
  text-align: center;
  color: #ffffff;
  z-index: 99999;
  top:50%;
  margin-top:-150rpx; 
}
.user-box{
  width: 100%;
  height: 145rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
}
.user-img{
  width: 120rpx;
  height: 120rpx;
  border-radius:50%;
  font-size: 34rpx;
  color: #000000;
}
.user-name-box{
  min-width:auto;
  max-width: 400rpx;
  height: 120rpx;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  margin-left: 5rpx;
  color: #ffffff;
}
.bottom{
  width: 100%;
  height: 90rpx;
  display: flex;
  position: absolute;
  top:50%;
  left: 100rpx;
  margin-top:75rpx; 
}
.left{
  width:270rpx;
  height: 90rpx;
  text-align: center;
  line-height: 90rpx;
}
