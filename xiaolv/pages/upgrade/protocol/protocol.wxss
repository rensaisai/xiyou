.index{
  width: 100%;
  height: auto;
  padding-bottom:20rpx; 
}
.logo-box{
  width: 100%;
  height: 256rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.icon-box{
  width: 140rpx;
  height: 140rpx;
}
.box{
  width: 100%;
  height: auto;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.brief{
  font-size: 26rpx;
  color: #666666;
}
.title{
  width: 100%;
  height: 60rpx;
  border-bottom:1rpx solid #E9E9E9;
  font-size: 34rpx;
  font-weight: 600;
  padding-left:20rpx; 
}
.subtitle{
  width: 100%;
  height: 88rpx;
  font-size:30rpx;
  font-weight: 600;
  line-height: 130rpx;
}