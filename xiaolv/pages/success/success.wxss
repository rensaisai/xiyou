@import '/pages/upgrade/success/success.wxss';
.index{
  width: 100%;
  height: 100%;
  background: #ffffff;
}
.success-box{
  width: 100%;
  height: 516rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.process-box{
  width: 100%;
  height: 150rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
}
.process{
  width: 100rpx;
  height: 150rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}
.arrow{
  width: 96rpx;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.jiantou{
  width: 26rpx;
  height: 18rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover
}
.process-icon1{
  width: 100rpx;
  height: 100rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.process-text{
  font-size: 24rpx;
  color: #333333;
  font-weight: 600;
}
.process-icon2{
  width: 100rpx;
  height: 100rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.process-icon3{
  width: 100rpx;
  height: 100rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.process-icon4{
  width: 100rpx;
  height: 100rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.order-details{
  width: 95%;
  height: 88rpx;
  background: #E84A2E;
  border-radius: 8rpx;
  margin: 75rpx auto 0;
  font-size: 30rpx;
  color: #ffffff;
  line-height: 88rpx;
  text-align: center;
}
.contact{
  width: 100%;
  height: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
  font-weight: 600;
  margin-top: 28rpx;
}