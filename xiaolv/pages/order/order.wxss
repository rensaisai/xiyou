@import '/pages/progect/project.wxss';
.consignee-box{
  width: 100%;
  height: 110rpx;
  background: #ffffff;
  padding: 0 20rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.consignee-box1{
  margin-top: 2rpx;
}
.consignee{
  width: 100%;
  height: 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 30rpx;
  color: #000000;
  font-weight: 600;
}
.consignee-icon{
  display: flex;
  align-items: center;
}
.icon{
  width: 30rpx;
  height: 30rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.consignee-name{
  margin-left: 27rpx;
}
.icon1{
  width: 30rpx;
  height: 30rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.icon2{
  width: 30rpx;
  height: 30rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.line{
  width: 100%;
  height: 8rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.pay-box{
  width: 100%;
  height: auto;
  margin-top:5rpx; 
  padding: 0 20rpx;
  box-sizing: border-box;
  background: #ffffff;
}
.pay-icon-box{
  width: 100%;
  height: 60rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.weixin-icon{
  width: 60rpx;
  height: 60rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.pay{
  padding: 0;
}
.pay-name{
 margin-left: 0;
 font-size: 30rpx;
 font-weight: 600;
 color: #000000;
}
.icon-box{
  display: flex;
  align-items: center;
}
.weixin-name{
  font-size: 30rpx;
  color: #000000;
  margin-left: 20rpx;
}
.selected-btn{
  width: 38rpx;
  height: 38rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.pay-price{
  font-size: 30rpx;
  color: #E84A2E;
  font-weight: 600;
}
.envelope{
  width: 100%;
  height: 110rpx;
  background: #ffffff;
  margin-top: 2rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 20rpx;
  box-sizing: border-box;
  font-size: 30rpx;
  color: #000000;
  font-weight: 600;
}
.envelope-right-box{
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: #E8862E;
  font-weight: 500;
}
.invoice{
  color: #7A7A7A;
}
.pay-details-box{
  width: 100%;
  height: 508rpx;
  background: #ffffff;
  margin-top: 5rpx;
  padding: 20rpx 20rpx 0;
  box-sizing: border-box; 
}
.pay-details{
  width: 100%;
  height: 70rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;
  color: #000000;
  font-weight: 600;
}
.goods-price{
  color: #E84A2E;
}
.price-box{
  width: 100%;
  height: 170rpx;
  display: flex;
  flex-direction: row-reverse;
  align-items: center;
  font-size:34rpx;
  color: #000000;
  font-weight: 600; 
}
.price{
  font-size: 38rpx;
  color: #E84A2E;
  font-weight: 650;
}
.btn{
  position: fixed;
  bottom: 0rpx;
  width: 95%;
  height: 88rpx;
  background: #E84A2E;
  font-size:30rpx;
  color: #ffffff;
  font-weight: 600; 
  border-radius:8rpx; 
  line-height: 88rpx;
}
.maintenance-box{
  padding: 0 0 0 20rpx;
  box-sizing: border-box;
  display: flex;
  flex-wrap: wrap-reverse;
  flex-direction: column;

}
.maintenance-list{
  width: 95%;
  padding-right: 20rpx;
  box-sizing: border-box;
}