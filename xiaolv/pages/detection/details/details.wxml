<view class="index">
   <view class="title-box">
      <image src="/image/logo.png" style='width:80rpx;height:80rpx;'></image>
      <view class="title">检测信息</view>
   </view>
      <view class="box">
       <view class="box-left">车牌号:</view>
       <view class="box-right">{{list.userCarNo}}</view>
   </view>
   <view class="box">
       <view class="box-left">检测时间:</view>
       <view class="box-right">{{list.createTime}}</view>
   </view>
   <view class="box">
       <view class="box-left">服务门店:</view>
       <view class="box-right">{{list.repairName}}</view>
   </view>
   <view class="box">
       <view class="box-left">技工:</view>
       <view class="box-right">{{list.technicianName}}</view>
   </view>
    <view class="box" wx:for="{{list.projects}}" wx:key="this">
       <view class="box-left">{{item.name}}</view>
       <view class="box-right">{{item.status1}}</view>
   </view>
   <view class="box">
       <view class="box-left">检测建议:</view>
       <view class="box-right">{{list.proposal}}</view>
   </view>
</view>

