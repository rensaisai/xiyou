.nav-boxs{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100rpx;
  background: #ffffff;
}
.navs{
  width: 50%;
  height: 99rpx;
  display: inline-block;
  text-align: center;
  line-height: 99rpx;
  border-bottom:1rpx solid #ffffff; 
}
.nav-selected{
  font-weight: 800;
  border-bottom:1rpx solid #E84A2E; 
}
.block{
  widows: 100%;
  height: 100rpx;
  background: #ffffff;
}
.list-box:first-child{
  margin-top: 0;
}
.list-box{
  width: 100%;
  height: 100rpx;
  background: #ffffff;
  padding: 0 20rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  margin-top: 1rpx;
}
.list{
  width: 25%;
  height: 100rpx;
  /* background: rebeccapurple; */
  padding: 0 20rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-wrap:wrap;
  font-size: 24rpx;
}
.index{
  width: 100%;
  height: auto;
}