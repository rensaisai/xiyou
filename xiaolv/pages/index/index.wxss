@import "/pages/common/index.wxss";
.index{
  width: 100%;
  height: auto;
}
.header-box{
  width: 100%;
  height: 88rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.left{
  width: 130rpx;
  height: 88rpx;
  display: flex;
  align-items: center;
}
.city{
  width: 100rpx;
  height: 88rpx;
  line-height: 88rpx;
  font-size:32rpx;
  color: #000000;
  overflow: hidden;
}
.icon-box{
  width: 20rpx;
  height: 20rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
  margin-left: 10rpx;
}
.top-searchbox{
  position: relative;
  padding-left: 46rpx;
  box-sizing: border-box; 
  width: 396rpx;
  height: 56rpx;
  background: #E7E7E7;
}
.search{
  position: absolute;
  left: 230rpx;
  width: 30rpx;
  height: 30rpx;
  background:url(data:image/png;base64,) no-repeat center center/cover;
}
.message-box{
  width: 44rpx;
  height: 44rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.swip{
  width: 100%;
  height: 300rpx;
}
.slide-image{
  width: 100%;
  height: 300rpx;
}
.nav{
  width: 100%;
  height: 220rpx;
  display: flex;
  justify-content: space-between;
  background: #ffffff;
}
.nav-box{
  width: 25%;
  height: 220rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  flex-direction: column;
  padding: 30rpx 0;
  box-sizing: border-box;
}
.nav-text{
  font-size: 22rpx;
  color: #000000;
}
.nav-img{
  width: 100rpx;
  height: 100rpx;
}
.vehicle-box{
  width: 100%;
  height: 150rpx;
  background: #ffffff;
  margin-top: 20rpx;
  display: flex;
  justify-content: space-between;
  padding: 0 20rpx;
  box-sizing: border-box;
  align-items: center;
}
.selectcar-img {
  width: 50px;
  height: 50px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.vehicle-information{
  width: 80%;
  height: 90rpx;
  margin-left: 10rpx;
}
.vehicle-informations{
  width: 80%;
  height: 90rpx;
  display: flex;
  align-items: center;
  margin-left: 10rpx;
  font-weight: 600;
  font-size: 34rpx;
}
.return{
  width: 16rpx;
  height: 25rpx;
}
.carName-box{
  display: flex;
  align-items: center;
  font-size:34rpx;
  color: #000000;
  font-weight: 600; 
}
.license-plate{
  width: 114rpx;
  height: 20rpx;
  border: 1rpx solid #E84A2E;
  border-radius:4rpx; 
  font-size: 16rpx;
  text-align: center;
  line-height: 20rpx;
  color: #E84A2E;
  margin-left: 10rpx;
}
.displacement{
  font-size: 24rpx;
  color:rgba(0,0,0,0.6); 
  font-weight: 600;
}
.list-box{
  width: 100%;
  height: 196rpx;
  background: #ffffff;
  padding: 0 20rpx;
  margin-top: 2rpx; 
  box-sizing: border-box;
  display: flex;
  align-items: center;
}
.list-img{
  width: 136rpx;
  height: 136rpx;
  border-radius:6rpx; 
}
.list-right{
  width: 80%;
  height: 196rpx;
  padding: 20rpx 0 20rpx 20rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction:column;
  justify-content: space-between;
}
.list-name{
  width: 100%;
  height: 36rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.garage-name{
  width: 78%;
  height: 36rpx;
  font-size: 36rpx;
  line-height: 36rpx;
  color: #000000;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.mechanics-img{
  width: 111rpx;
  height: 30rpx;
}
.evaluation{
  font-size: 26rpx;
  line-height: 36rpx;
  color: #7C7C7C;
}
.positioning-box{
  width: 150rpx;
  height: 88rpx;
  display: flex;
  justify-content:space-around;
  align-items: center;
}
.address{
  width: 95%;
  height: 36rpx;
  font-size:26rpx;
  line-height: 36rpx;
  color: #7A7A7A; 
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.stores-box{
  width: 100%;
  height: 90rpx;
  background: #ffffff;
  margin-top:20rpx; 
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.service{
  font-size: 32rpx;
  color: #000000;
}
.more{
  font-size: 28rpx;
  color: #7A7A7A;
}
.positioning{
  width: 22rpx;
  height: 22rpx;
} 
.points{
  color: #E84A2E;
}
.advertising-img{
  width: 100%;
  height: 210rpx;
}
.commodity_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 1000;
}
.commodity_attr_box {
  position: fixed;
  width: 80%;
  height: 300rpx;
  top: 50%;
  left: 50%;
  margin-top:-150rpx;
  margin-left: -40%;
  z-index: 99999;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  background: #ffffff;
  border-radius:10rpx; 
}
.open{
  width: 100%;
  height: 200rpx;
  padding: 50rpx 60rpx;
  box-sizing: border-box;
  text-align: center;
  line-height: 50rpx;
  font-size: 30rpx;
}
.btn{
  display: flex;
  width: 100%;
  height: 100rpx;
  border-top:1rpx solid #eeeeee; 
}
button{
  width: 50%;
  margin-top:0rpx;
  margin-bottom:0rpx; 
  padding-left: 0;
  padding-right: 0;
  background: #ffffff; 
}
button::after{
  border: none;
}
.dialogue{
  width: 44rpx;
  height: 44rpx;
  background: #F3F3F3;
}
.advertising-pop{
  position: fixed;
  top:50%;
  left: 50%;
  margin-top: -346rpx;
  margin-left: -240rpx;
  width: 480rpx;
  height: 692rpx;
  /* background: #ffffff; */
  border-radius:20rpx; 
  z-index: 999999;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.shut-btn-box{
  width: 100%;
  height: 76rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.shut-btn{
  width: 76rpx;
  height: 76rpx;
}