.index{
  width: 100%;
  height: auto;
}
.template-btn-box{
  width: 100%;
  height: 80rpx;
  display: flex;
  flex-direction: row-reverse;
  align-items: center;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.template-btn{
  width: 174rpx;
  height: 50rpx;
  border-radius:25rpx;
  background: rgba(225, 225, 225,0.8);
  font-size: 24rpx;
  color: rgba(0, 0, 0,0.8);
  text-align: center;
  line-height: 50rpx;
  font-weight: 600;
}
.add-img-box{
  width: 100%;
  height: 290rpx;
  background: url("https://www.cmspq.xyz/cmsweb/imgs/SmallProcedures/15.png") no-repeat center center/cover;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
  color: #ffffff;
}
.add-box{
  width: 210rpx;
  height: 80rpx;
  background: rgba(225, 225, 225,0.4);
  border-radius:40rpx; 
  line-height: 80rpx;
  margin-top: 20rpx;
  text-align: center;
}
.list-box{
  width: 100%;
  height: auto;
  padding: 0 0 0 20rpx;
  box-sizing: border-box;
  background: #ffffff;
}
.list{
  width: 100%;
  height: 100rpx;
  border-bottom:1rpx solid #EEEEEE;
  padding-right: 20rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  font-weight: 600;
}
.list-left{
  width: auto;
  height: 100rpx;
  display: flex;
  align-items: center;
}
.list-right{
  width: auto;
  height: 100rpx;
  display: flex;
  align-items: center;
  font-size:30rpx;
  color: rgba(0, 0, 0,0.7); 
}
.list-icon{
  width: 34rpx;
  height: 34rpx;
}
.list-text{
  font-size: 34rpx;
  color: #000000;
  margin-left: 34rpx;
  line-height: 100rpx;
}
.submit-btn-box{
  width: 100%;
  height: 188rpx;
  display: flex;
  align-items: center;
  justify-content: space-evenly;
  background: #ffffff;
}
.submit-btn{
  width: 330rpx;
  height: 88rpx;
  background:rgba(232,74,46,1);
  font-size: 30rpx;
  color: #ffffff;
  line-height: 88rpx;
}
.submit{
  background:rgba(241,141,123,1);
}