}
button::after {
  border: none;
}
button{
  background-color: #fff;
  border: none;
  padding: 0;
  margin: 0;
  border-radius:0; 
  font-size: 30rpx;
  text-align: center;
}
.btn-l{
  width: 50%;
  height: 88rpx;
  background:#ffffff;
}
.btnul{
  border-radius:0 0 0 20rpx; 
  border-right:1rpx solid #eeeeee; 
}
.btnus{
  border-radius:0 0 20rpx 0;
  color: #fd4200;
}
