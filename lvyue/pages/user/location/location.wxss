
 .index{
   width: 100%;
   height: auto;
   padding-bottom: 100rpx;
 }
.indexs-box{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: auto;
  background-color: #ea294b;
  font-size: 30rpx;
  color: #ffffff;
  font-weight: 600;
  text-align: center;
  line-height: 100rpx;
}
.index-box{
  width: 100%;
  height: auto;
  background: #ffffff;
}
.location-index{
  width: 100%;
  background: #ffffff;
  margin-top:5rpx; 
  padding: 0 20rpx;
  box-sizing: border-box;
}
.location-box{
  font-size: 32rpx;
  color: #000000;
  font-weight: 600;
}
.location-box1{
  font-size: 30rpx;
  color: #000000;
  font-weight: 600;
  float: right;
}
.location-box2{
  font-size: 30rpx;
  color: #808080;
  padding: 35rpx 0;
}
.location-label{
  display: flex;
  justify-content: space-between;
  width: 100%;
  height: 40px;
  border-top:1rpx solid #e6e6e6; 
}
.weui-cell{
  display: flex;
  align-items: center;
  padding: 0 20rpx 0;
  /* font-size:14px; 
  line-height: 40rpx; */
}
.location-right{
  display: flex;
  justify-content: space-around;
  align-items: center;
  font-size: 18px;
  color: rgb(22, 22, 22);
  width: 40%;
  height: 40px;
}
.location-left{
  font-size: 16px;
}
.yuan{
  width: 70rpx;
  height: 80rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.weui-cell__hd{
  width: 45rpx;
  height: 45rpx;
  border: 1rpx solid #808080;
  border-radius:50%;
}
.weui-check__labels{
  width: 50rpx;
  height: 50rpx;
  border: none;
  background:url(data:image/png;base64,)no-repeat center center/cover;
}