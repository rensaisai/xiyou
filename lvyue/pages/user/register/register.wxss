.index{
  width: 100%;
  height: 100%;
  background: #ffffff;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding: 100rpx 0;
  box-sizing: border-box;
}
.log-box{
  width: 100%;
  height: 160rpx;
}
.log{
  width: 160rpx;
  height: 160rpx;
  border-radius:50%;
  margin: 0 auto;
  background: url(data:image/png;base64,) no-repeat center center/cover; 
}
.login-btn-box{
  width: 100%;
  height: 240rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.btn{
  width: 95%;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  font-size: 30rpx;
  color: #ffffff;
  font-weight: 600;
}
