.index{
  width: 100%;
  height: 100%;
  padding: 20rpx;
  box-sizing: border-box;
}
.box{
  width: 100%;
  height: 412rpx;
  background: #ffffff;
  border-radius:10rpx; 
  padding: 0 20rpx;
  box-sizing: border-box;
}
.title-box{
  width: 100%;
  height: 120rpx;
  border-bottom:1rpx solid #E9E9E9; 
}
.title{
  width: 100%;
  height: 60rpx;
  font-size: 32rpx;
  color: #5E5E5E;
  line-height: 60rpx;
}
.headings{
  width: 100%;
  height: 60rpx;
  font-size: 28rpx;
  color: #868686;
  line-height: 60rpx;
}
.license{
  width: 100%;
  height: 75rpx;
  font-size: 28rpx;
  color: #5E5E5E;
  line-height: 75rpx;
}
.img-box{
  width: 246rpx;
  height: 191rpx;
  border: 1rpx dashed #F1F1F1; 
 
}
