  flex-direction: column;
}
.seek{
  width: 75%;
  height: 80rpx;
  background-color: #ffffff;
  border-radius:32rpx; 
  padding-left: 30px;
  box-sizing: border-box;
  font-size: 28rpx;
}
.phone{
  width: 45rpx;
  height: 45rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.seek-box{
  position: absolute;
  top:28rpx;
  left: 126rpx;
  width: 36rpx;
  height: 36rpx;
  background:url(data:image/png;base64,) no-repeat center center/cover;
}
.swiper-box{
  width: 100%;
  height: 369rpx;
}
.img{
  width: 100%;
}
.headline{
  width: 100%;
  height: 118px;
  display: flex;
  flex-direction:column;
  justify-content: center;
  align-items: center;
  background: #ffffff;
}
.headlines{
  margin-top: 20rpx;
}
.text{
  color: #e4183a;
  font-size: 42rpx;
}
.text-box{
  width: 70%;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.text-left{
  width: 80rpx;
  height: 2rpx;
  background: #AEAEAE;
  border-radius:1rpx; 
}
.text-sg{
  color: #aeaeae;
  font-size: 26rpx;
}
.scrolls{
  width: 100%;
  height:370rpx;
  white-space: nowrap;
  overflow-x:scroll; 
  padding-right: 20rpx;
  box-sizing: border-box;
  background: #ffffff;
  -webkit-overflow-scrolling: touch;
}
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}
.imgs-box{
 position: relative;
 display: inline-block;
 width: 85%;
 height: 340rpx;
 margin-left:20rpx; 
}
.imgs{
  width: 100%;
  height: 340rpx; 
  border-radius:15rpx;
 } 
.sign-img{
  position: absolute;
  top: 0;
  left: 0;
  width: 240rpx;
  height: 50rpx;
  z-index: 9999; 
  font-size: 24rpx;
  color: #ffffff;
  text-align: center;
  line-height: 50rpx;
   border-radius:15rpx 0 15rpx 0;
  background: linear-gradient(to right, #ff2d5f , #ff6155); /* 标准的语法 */
}
.sign-second{
  position: absolute;
  border-radius: 0 0 15rpx 15rpx; 
  bottom: 0;
  width: 100%;
  height: 72rpx;
  background: rgba(0, 0, 0,0.4);
  display: flex;
  justify-content: space-around;
  align-items: center;
  font-size: 30rpx;
  color: #ffffff;
}
.name-box{
  width: 50%;
  height: 72rpx;
  line-height:72rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.advertising{
  width: 100%;
  height: 180rpx;
  padding: 20rpx;
  box-sizing: border-box;
  background: #ffffff;
  display: inline-block;
}
.advertising-img{
  width: 100%;
  height: 140rpx; 
  border-radius:30rpx;
  display: inline-block;
}
.Since-list{
  width: 100%;
  height: auto;
  background: #f1f1f3;
}
.list{
  position: relative;
  width: 100%;
  height: auto;
  padding: 0 20rpx;
  box-sizing: border-box;
  background: #ffffff;
  margin-top: 20rpx;
}
.list:nth-child(1){
  margin-top: 0rpx;
}
.list-img{
  width: 100%;
