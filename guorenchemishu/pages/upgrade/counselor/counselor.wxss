.index{
  width: 100%;
  height: 100%;
}
.text-name{
  font-size:24rpx; 
}
.header-box{
  width: 100%;
  height: 340rpx;
  background: url(https://www.cmspq.xyz/cmsweb/imgs/SmallProcedures/13.png) no-repeat center center/cover;
}
.top-box{
  width: 100%;
  height: 142rpx;
  display: flex;
  justify-content: space-between;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.header-img{
  width: 142rpx;
  height: 142rpx;
  border-radius:71rpx; 
}
.header-left{
  width: 70%;
  height: 142rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding: 0 10rpx;
  box-sizing: border-box;
}
.name{
  height: 71rpx;
  color: #ffffff;
  font-size:38rpx; 
  display: flex;
  align-items: center;
}
.name-box{
  width: 116rpx;
  height: 36rpx;
  background: #e39f6d;
  border-radius: 18rpx;
  margin-left:20rpx;
  font-size: 22rpx;
  color: #ffffff;
  text-align: center;
  line-height: 36rpx;
}
.number{
  font-size: 28rpx;
  color:rgba(255,255,255,0.8);
  line-height: 28rpx;
}
.header-bottom{
  position: relative;
  width: 100%;
  height: 198rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.xian{
  width: 80%;
  height: 3rpx; 
  border: 1rpx dashed #99A4B1; 
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
}
.sign{
  width: 30rpx;
  height: 30rpx;
}
.sign:first-child{
  margin-left:-15rpx; 
}
.sign:last-child{
   margin-right:-15rpx; 
}
.img{
  z-index: 99;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.imgs{
  z-index: 999;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.xian1{
  margin-top:48rpx; 
  width: 80%;
  height: 3rpx; 
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
}
.xian1-text{
font-size: 26rpx;
color: #ffffff;
}
.xian1-text:first-child{
margin-left:-50rpx; 
}
.xian1-text:last-child{
  margin-right:-50rpx;
}
.xian2{
  position: absolute;
  height: 8rpx;
  background: #ffffff;
  border-radius:5rpx; 
  top: 72rpx;
  left: 72rpx;
}
.earnings-box{
  width: 100%;
  height: 146rpx;
  display: flex;
  justify-content: space-between;
}
.earnings{
  width: 33%;
  height: 146rpx;
  background: #ffffff;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.commission{
  font-size: 36rpx;
  font-weight: 600;
  color: #000000;
}
.commission-name{
  font-size:24rpx;
  color:rgba(0,0,0,0.6); 
}
.instrument-box{
  width: 100%;
  height: 652rpx;
  margin-top:40rpx; 
}
.instrument-title{
  font-size: 32rpx;
  color: #000000;
  line-height: 99rpx; 
  background: #ffffff;
  padding-left:20rpx; 
}
.boxes{
  width: 100%;
  height: 552rpx;
  display: flex;
  flex-wrap:wrap;
  justify-content: space-between;
  align-items: center;
}
.list-box{
  width: 49.865%;
  height: 273rpx;
  background: #ffffff;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top:1rpx;

}
.list{
  width: 160rpx;
  height: 200rpx;
}
.list-icon{
  width: 159rpx;
  height: 159rpx;
  /* border: 1rpx dashed #2F3241; */
}

.list-text{
  font-size: 30rpx;
  color: #000000;
  text-align: center;
}
.code{
  width: 65rpx;
  height: 142rpx;
  display: flex;
  align-items: center;
}
.code-img{
  width: 65rpx;
  height: 65rpx;
}
.commodity_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 1000;
}
.commodity_attr_box {
  position: fixed;
  width: 100%;
  height: 820rpx;
  top: 50%;
  margin-top:-410rpx;
  z-index: 99999;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.swiper-item{
display: flex;
flex-direction: column;
justify-content: center;
align-items: flex-start;
overflow:unset;
height: 483rpx;
}
.share-box{
  width: 100%;
  height: 646rpx; 
  border-radius:8rpx;
}
.share-img{
  width: 430rpx;
  height: 570rpx;
  display: inline-block;
}
.active{
transform: scale(1.1);
transition:all .2s ease-in 0s;
}
.share-btn{
  width: 483rpx;
  height: 88rpx;
  background: #e31436;
  border-radius:44rpx;
  margin: 0 auto;
  font-size: 34rpx;
  text-align: center;
  line-height: 88rpx;
  color: #ffffff;
  font-weight: 600;  
  z-index: 99999;
}