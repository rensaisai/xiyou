.index{
  width: 100%;
  height: auto;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.shuffling-box{
  width: 100%;
  height: 300rpx;
}
.slide-image{
  width: 100%;
  height: 300rpx;
}
.title{
  width: 100%;
  height: 90rpx;
  color: #000000;
  font-size: 36rpx;
  font-weight: 600;
  line-height: 90rpx;
}
.cloud{
  width: 100%;
  height: 408rpx;
  background: url("https://www.cmspq.xyz/cmsweb/imgs/SmallProcedures/11.png") no-repeat center center/cover;
}
.discounts{
  width: 100%;
  height: 100rpx;
  font-size: 36rpx;
  color: #000000;
  line-height: 120rpx;
  font-weight: 600;
}
.coupon{
  width: 100%;
  height: 190rpx;
  margin-top: 20rpx;
  display: flex;
  background: url("https://www.cmspq.xyz/cmsweb/imgs/SmallProcedures/12.png") no-repeat center center/cover;
}
.coupon-left{
  width: 71%;
  height: 190rpx;
  padding: 0 20rpx 0 20rpx;
  box-sizing: border-box;
}
.coupon-left-top{
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
}
.icon{
  width: 28rpx;
  height:32rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.text{
  min-width:auto;
  max-width: 48%;
  font-size: 36rpx;
  color: #000000;
  font-weight: 600;
  margin-left: 10rpx;
  line-height: 36rpx;
}
.activity{
  width: auto;
  height: 28rpx;
  border: 1rpx solid #E84A2E;
  border-radius:5rpx;
  font-size: 20rpx;
  color: #E84A2E;
  padding: 0 5rpx;
  box-sizing: border-box;
  text-align: center;
  line-height: 24rpx; 
  margin-left: 20rpx;
}
.coupon-bottom{
  width: 100%;
  height:110rpx;
}
.describe{
  font-size: 22rpx;
  color:#959595;
  line-height: 30rpx;
}
.coupon-right{
  width: 29%;
  height: 190rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.price{
  font-size: 48rpx;
  color: #ffffff;
  font-weight: 600;
}
.btn{
  width: 136rpx;
  height:40rpx;
  border-radius: 20rpx;
  background: rgba(255,255,255,0.8);
  font-size: 26rpx;
  color: #E84A2E;
  line-height: 40rpx;
  text-align: center;
}