@import '/pages/upgrade/coupon/coupon.wxss';
.block{
  width: 100%;
  height: 92rpx;
}
.index{
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.indexs{
  width: 100%;
  height: auto;
}
.inde{
  width: 100%;
  height: 100%;
  padding: 0 20rpx;
  box-sizing: border-box;
  overflow: hidden;
}
.nav{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 92rpx;
  border-bottom:1rpx solid #E8E8E8; 
  display: flex;
  background: #ffffff;
  z-index: 999;
}
.nav-box{
  width: 50%;
  height: 92rpx;
  text-align: center;
  line-height: 91rpx;
}
.hot{
  border-bottom:1rpx solid #E84A2E; 
  color:#E84A2E;
}
.title{
  width: 100%;
  height: 154rpx;
  line-height: 154rpx;
  font-size:36rpx;
  color: #000000; 
}
.input{
  width: 100%;
  height: 88rpx;
  border:1rpx solid #CED5E0;
  border-radius:8rpx; 
  padding: 0 20rpx;
  box-sizing: border-box;
}
.inputs{
  width: 65%;
  height: 88rpx;
}
