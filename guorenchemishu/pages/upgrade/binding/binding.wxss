.across{
  width: 100%;
  height: 88rpx;
  border-bottom:1rpx solid #F5F5F5;  
  padding:0 20rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center; 
}
.across:nth-child(1){
  border-top:1rpx solid #F5F5F5;
}
.across:last-child{
  border-bottom:0rpx solid #F5F5F5; 
}
.across-text{
  color: #E84A2E;
  font-size: 28rpx;
}
.textl{
  color: #1A1F24;
  font-size: 28rpx; 
}
.across-img{
  width: 32rpx;
  height: 32rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}