.heider{
  width: 100%;
  height: 88rpx;
  background: rgba(254,249,241,1);
  padding: 0 20rpx;
  box-sizing: border-box;
  font-size: 30rpx;
  font-weight: 600;
  color: #000000;
  line-height: 88rpx;
}
.nav-box{
  width: 100%;
  height: 125rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.before{
  width: 110rpx;
  height: 125rpx;
  display: flex;
  align-items: center;
}
.time{
  font-size: 30rpx;
  color: #000000;
  font-weight: 600;
}
.arrow{
  width: 26rpx;
  height: 32rpx;
}
.text{
  font-size: 28rpx;
  color: #77777D;
  line-height: 125rpx;
  font-weight: 600;
}
.box{
  width: 100%;
  height: auto;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.slect{
  background: #E84A2E;
  color: #ffffff!important;
}
.time-box{
  width: 124rpx;
  height: 58rpx;
  border: 1rpx solid #E84A2E;
  border-radius:6rpx;
  font-size: 32rpx;
  text-align: center;
  line-height: 58rpx;
  color: #E84A2E;
  font-weight: 600; 
  display: inline-block;
  margin-right: 18rpx;
  margin-top: 18rpx;
}
.time-borde{
  width: 124rpx;
  height: 58rpx;
  border: 1rpx solid #D5D5D5;
  border-radius:6rpx;
  font-size: 32rpx;
  text-align: center;
  line-height: 58rpx;
  color: #ABABAB;
  font-weight: 600; 
  display: inline-block;
  margin-right: 18rpx;
  margin-top: 18rpx;
  background: #F0F0F0;
}
.quantum:nth-child(5n){
   margin-right: 0rpx;
}