/* @import "/pages/cars/carselect/fct/fctlist.wxss"; */
.index{
  width: 100%;
  height: auto;
  background: #ffffff;
}
.letter{
  width: 100%;
  height: 40rpx;
  background: #F2F2F2;
  font-size:22rpx;
  color: rgba(0,0,0,0.6); 
  padding: 0 0 0 20rpx;
  box-sizing: border-box;
  line-height: 40rpx;
}
.list-box{
  width: 100%;
  height: 88rpx;
  border-bottom:1rpx solid #F0ECEC; 
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding:0 20rpx;
  box-sizing: border-box;
}
.fctName{
  font-size: 30rpx;
  color: #000000;
  font-weight: 600;
}
.jiantou{
  width: 22rpx;
  height: 26rpx;
}