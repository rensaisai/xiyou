@import "/pages/common/index.wxss";
.box{
  width: 100%;
  height: 120rpx;
  background: #ffffff;
  padding: 0 20rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  margin-top: 2rpx;
  font-size: 28rpx;
  color: #000000;
  font-weight: 600;
}
