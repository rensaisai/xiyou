@import "/pages/common/index.wxss";
.box{
  width: 100%;
  height: 160rpx;
}
.box-text{
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  font-size:30rpx;  
  padding-left:20rpx; 
  box-sizing: border-box;
  
}
.box-input{
  width: 100%;
  height: 90rpx;
  border-top:1rpx solid #dfdfdf; 
  border-bottom:1rpx solid #dfdfdf;
  padding-left:20rpx;
  background: #ffffff; 
  box-sizing: border-box;
  display: flex;
}
.sweep-box{
  width: 15%;
  height: 90rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.sweep{
  width: 50%;
  height: 50%;
}
.right{
  display: flex;
  align-items: center;
}
.select{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-right:20rpx; 
  box-sizing: border-box;
}
.fanhui{
  font-size: 26rpx; 
  color: #666666;
}
.jiantou{
  width:28rpx;
  height: 28rpx;
}
.commodity_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9998;
  color: #fff;
}
.commodity_attr_box {
  width: 100%;
  position: fixed;
  bottom: 0;
  left: 0;
  height: 720rpx;
  z-index: 9999;
  background: #ffffff;
  padding-bottom: 100rpx;
}
.weui-cell__hd{
  width: 44rpx;
  height: 44rpx;
  border: 1rpx solid #808080;
  border-radius:50%;
  padding: 1rpx;
}
.weui-check__labels{
  width: 50rpx;
  height: 50rpx;
  border: none; 
  background:url(data:image/png;base64,)no-repeat center center/cover;
}
.weuixi-right{
  width: 52rpx;
  height: 52rpx;
}
.placeholdText{
 z-index:11;
}
.index-bd{
  width: 100%;
  height: 100%;
  overflow-x: hidden;
  overflow-y: scroll;
  padding-top: 40px; 
}
.boxl{
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.anmit{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 40px;
  background-color: #ffffff;
  border-bottom: 1rpx solid #eeeeee;
  padding-left: 20rpx;
  line-height: 40px;
}
.kind-list-item-hd{
  padding: 20rpx;
  box-sizing: border-box;
}
.kind-list-item-box{
  width: 100%;
  height: auto;
  display: flex;
  align-items: center;
}
.kind-list-text{
  margin-left:10rpx; 
}
.left{
  width: 85%;
  height:90rpx;
  padding-right:10rpx;
  box-sizing: border-box; 
  display: flex;
  align-items: center;
  flex-wrap:wrap;
}