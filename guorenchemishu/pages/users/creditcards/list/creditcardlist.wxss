@import "/pages/common/index.wxss";
.index{
  width: 100%;
  height: auto;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.btn{
  position: fixed;
  bottom: 20rpx;
  width: 95%;
  height: 88rpx;
  border-radius:8rpx; 
  background:#E84A2E; 
  font-size: 26rpx;
  color: #ffffff;
  text-align: center;
  line-height: 88rpx;
}
.add{
  margin-left: 10rpx;
  font-size:32rpx; 
}
.btn-img{
  width: 44rpx;
  height: 44rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.bank-box{
 position:relative; 
 width: 100%;
 height: 224rpx;
 margin-top:20rpx; 
}
.box{
  position: absolute;
  top:32rpx;
  width: 100%;
  height: 70rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 40rpx 0 120rpx; 
  box-sizing: border-box;
}
.box-left{
 display: flex;
 flex-direction: column;
}
.bank-name{
 height: 35rpx;
 font-size:30rpx;
 color:rgba(255,255,255,0.9);
 line-height: 33rpx;
}
.name{
height: 35rpx;
font-size:26rpx;
color:rgba(255,255,255,0.8);
line-height: 33rpx;
}
.untie-btn{
  width: 100rpx;
  height: 46rpx;
  background: url(data:image/png;base64,) no-repeat center center/cover;
}
.card{
  position: absolute;
  bottom: 30rpx;
  width: 100%;
  height: 42rpx;
  font-size:42rpx;
  color: #ffffff;
  font-weight:bold; 
  line-height: 42rpx;
  margin-left: 120rpx;
}